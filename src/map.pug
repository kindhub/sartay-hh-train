mixin map-city(name, nameTransliteration, area, filterArea)
	.map-city.city-block.vacancy-btn(class=("map-city--" + nameTransliteration) data-area=area data-city=("г. " + name))
		.map-city__name!=name
		| <hht:vacancies-filter area="#{filterArea}"/>

mixin map-city-option(name, filterArea)
	.swiper-slide
		.vacancies-map-select__option.city-block(data-city=("г. " + name))!=name
		| <hht:vacancies-filter area="#{filterArea}"/>


.vacancies-map__container
	.vacancies-map-select__container
		.vacancies-map-select__label Выберите населенный пункт
		.vacancies-map-select.vacancies-map-select--noborder
			.vacancies-map-select__slider-container.swiper-container.swiper-container-scroll
				.swiper-wrapper
					+map-city-option("Актау", "2")
					+map-city-option("Атырау", "153,3034")
					+map-city-option("Актобе", "154")
					+map-city-option("Алматы", "6287")
			
